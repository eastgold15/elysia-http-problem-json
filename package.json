{
  "name": "@pori15/elysia-http-problem-json",
  "version": "0.2.1",
  "type": "module",
  "module": "src/index.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.js",
    "node": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky || true",
    "prepublishOnly": "HUSKY=0 && bun test && bun tsc",
    "build1": "rm -rf dist && bun build src/index.ts --outdir ./dist --target bun --minify --external elysia && tsc",
    "build": "pkgroll",
    "test": "bun test",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "release": "bun pm version patch && bun  publish --access public"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.12",
    "@elysiajs/eden": "^1.4.4",
    "@types/bun": "latest",
    "husky": "^9.1.7",
    "ultracite": "7.1.1",
    "zod": "^4.1.12",
    "pkgroll": "^2.22.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "elysia": ">=1.4.11"
  },
  "keywords": [
    "elysia",
    "problem",
    "json",
    "http",
    "bun"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pori15/elysia-http-problem-json"
  }
}
